TestSuites
	TestSuite 'First Page'
		Test 'test1 ' (result=TRUE)
			openBrowser
			openpage 'http://www.imt-atlantique.fr/fr'
			verify link 'Toutes les actualités'
		end
		
		Test 'test2 ' (result=TRUE)
			openBrowser
			openpage 'http://www.imt-atlantique.fr/fr'
			clickOn link 'Toutes les actualités'
			verify link 'Acceuil'
		end
		
		Test 'test3 ' (result=TRUE)
			openBrowser
			openpage 'http://www.imt-atlantique.fr/fr'
			verify link 'Toutes les actualités'
			clickOn image 'Toutes les actualités' 'alt=Acceuil'
			verify link 'Toutes les actualités' 
		end
		
	end
	
	TestSuite 'ResearchPage'
		Test 'test4 ' (result=TRUE)
				openBrowser
				openpage 'http://www.imt-atlantique.fr/fr/rechercher'
				insert searchfield 'searchfield1' 'DonalTrump'
				clickOn button 'Appliquez les filtres'
				verify text 'Aucun résultat ne correspond à votre recherche' 
		end
		
		Test 'test6 ' (result=FALSE)
				openBrowser
				openpage 'http://www.imt-atlantique.fr/fr/rechercher'
				insert searchfield 'searchfield1' '2007'
				choosecombo combobox 'combobox1' { 'Lemoisdernier' }
				clickOn button 'Appliquer les filtres'
		end
		
		Test 'test8 ' (result=FALSE)
				openBrowser
				openpage 'http://www.imt-atlantique.fr/fr/rechercher'
				insert searchfield 'searchfield1' '2007'	 			
				clickOn button 'Appliquer les filtres'
				choosecombo combobox 'combobox1' { 'Lemoisdernier' }
				clickOn button 'Appliquer les filtres'
		end
	end
	
	TestSuite 'FindCourses'
		Test 'test5 ' (result=TRUE)
				openBrowser
				openpage 'http://www.imt-atlantique.fr/fr/trouver-ma-formation'
				check checkbox 'all' FALSE 	
				check checkbox 'Anglais' TRUE
				check checkbox 'A domicile' TRUE
				clickOn button 'Appliquer les critères'
				verify text 'Aucune formation trouvée répondant à vos critères'

		end
	end
	
	TestSuite 'Navigation'
		Test 'test7 ' (result=TRUE)
				openBrowser
				openpage 'http://www.imt-atlantique.fr/fr'
				read text 'title1'
				openpage 'http://www.imt-atlantique.fr/fr/rechercher'
				insert searchfield 'searchfield1' 'title'
				clickOn button 'Appliquer les filtres'
				verify text 'http://www.imt-atlantique.fr/fr'
		end
	end
end